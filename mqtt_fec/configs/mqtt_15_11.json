{
    "nb_points": 95,

    "use_fec": true,

    "output": "results_15_11_impact_client",

    "design": "wps",

    "enable_log": false,

    "encoder": {
        "scheme": "rlc",
        "window_size": 20,
        "window_step": 2
    },

    "decoder": {
        "ge": {
            "k": {
                "min": 0.95,
                "max": 1.0
            },
            "p": {
                "min": 0.01,
                "max": 0.1
            },
            "h": {
                "min": 0.0,
                "max": 0.1
            },
            "r": {
                "min": 0.1,
                "max": 0.8
            }
        }
    },

    "mqtt": {
        "nb_clients": 6,
        "nb_messages": 100,
        "timeout": 120
    }

}
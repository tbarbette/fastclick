        linuxpath=/usr/src/linux
    fi
    if test ! -d "$linuxpath" -a "x$enable_linuxmodule" = xyes; then
        if test "x$enable_linuxmodule_default" = xyes; then
            linuxpath=NONE; enable_linuxmodule=no
        else
